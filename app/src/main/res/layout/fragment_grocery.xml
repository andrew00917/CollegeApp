                        android:layout_marginRight="5dp"
                        android:paddingTop="3dp"
                        android:text="8:00am - 10:00pm"
                        android:layout_toRightOf="@+id/img05"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
