                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/btn_grocery_laMexicanaMarket"
                    android:layout_marginTop="1dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_whitebackground"
                    android:padding="10dp">

                    <ImageView
                        android:id="@+id/img06"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/email"
                        android:layout_alignParentLeft="true"/>

                    <TextView
                        android:id="@+id/tv06"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dp"
                        android:text="@string/label_grocery_laMexicanaMarket"
                        android:textSize="18dp"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:layout_alignParentTop="true"
                        android:layout_toRightOf="@+id/img06"/>
                    <TextView
                        android:id="@+id/tvlaMexicanaMarket"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv06"
                        android:layout_marginRight="5dp"
                        android:paddingTop="3dp"
                        android:text="8:00am - 10:00pm"
                        android:layout_toRightOf="@+id/img06"/>

                    <TextView
